.tab-left{ 
 height: 80rpx;
 background: #f5f5f5;
 line-height: 80rpx;
 text-align: center;
 font-size: 32rpx;
 width:100%;
 margin-bottom: 40rpx;
} 
.tab-left view{ 
  border-bottom: solid 2rpx #e1e1e1; 
  width: 50%;
  float: left;
  box-sizing: border-box;
} 
.tab-left view.active{ 
  color: #026a19; 
  border-bottom: solid 4rpx #026a19; 
} 
.tab-right{ 
 line-height: 160%; 
} 
.tab-right .right-item{ 
 display: none; 
} 
.tab-right .right-item.active{ 
 display: block; 
}
#tab-hd01{
  border-right: 2rpx solid #e1e1e1;
}
.h130Feature{
  background: #f5f5f5;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.h130FeatureT{
  margin: 20rpx 0 20rpx 0; 
  line-height: 60rpx;
  height: 60rpx;
  color: #444;
}
.h130FeatureT text{
    border-bottom: 6rpx solid #ddd;
    padding-bottom: 6rpx;
    padding-right: 100rpx;
    font-size: 36rpx;
}
.h130FeatureH{
    display: block;
    font-size: 32rpx;
    margin-top: 10rpx;
    color: #555;
}
.h130FeatureInfo{
    display: block;
    font-size: 31rpx;
    color: #777;
}
.jixingTable td {
    border-bottom: 1px solid #ddd;
    padding: 8px 3px;
    text-align: left;
    padding-left: 5px;
}
.jixingTable table tr.shenhuibg td {
    background: #f3f3f3 none repeat scroll 0 0;
}
.eqName{
  font-size: 30rpx;
  margin-bottom: 10rpx;
}